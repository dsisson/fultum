{
    "manifest_version": 2,
    "name": "DOM Element Highlighter",
    "version": "1.0",
    "description": "Highlight DOM elements on mouseover",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
